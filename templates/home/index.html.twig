{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}
<div class="container">
    <ul class="list-group mt-5">
        {% for chat in chats %}
            <a href="{{ path('app_chat', { 'chat': chat.id }) }}" class="list-group-item list-group-item-action">{{ chat }}</a>
        {% endfor %}
    </ul>
</div>
{% endblock %}
